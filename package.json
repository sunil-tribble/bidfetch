{
  "name": "bidfetch",
  "version": "1.0.0",
  "description": "Open Opportunity Engine - $0 Data Acquisition System for Government Procurement",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node src/multi-source-server.js",
    "start:simple": "node src/production-server.js",
    "start:ts": "node dist/index.js",
    "start:dev": "tsx watch src/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "db:migrate": "tsx src/database/migrate.ts",
    "ingest:sam": "tsx src/services/sources/sam-gov/index.ts",
    "ingest:grants": "tsx src/services/sources/grants-gov/index.ts",
    "ingest:fpds": "tsx src/services/sources/fpds/index.ts",
    "worker": "tsx src/workers/index.ts",
    "scheduler": "tsx src/scheduler/index.ts",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run build:frontend"
  },
  "keywords": [
    "procurement",
    "government",
    "contracting",
    "bidding",
    "RFP"
  ],
  "author": "BidFetch Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.862.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/adm-zip": "^0.5.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^24.2.0",
    "@types/pg": "^8.15.5",
    "adm-zip": "^0.5.16",
    "axios": "^1.11.0",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "joi": "^18.0.0",
    "mammoth": "^1.10.0",
    "mongoose": "^8.17.0",
    "node-cron": "^4.2.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/bull": "^3.15.9",
    "@types/jest": "^30.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1"
  }
}
